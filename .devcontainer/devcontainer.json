{
    "name": "Misskey Webp Proxy",
    "dockerComposeFile": [
        "../compose.yaml"
    ],
    "service": "app",
    "workspaceFolder": "/home/vscode/workspace",
    "remoteUser": "vscode",
    "customizations": {
        "vscode": {
            "extensions": [
                "rust-lang.rust-analyzer",
                "vadimcn.vscode-lldb",
                "serayuzgur.crates",
                "tamasfe.even-better-toml",
                "mhutchie.git-graph",
                "GitHub.vscode-pull-request-github",
                "github.vscode-github-actions"
            ]
        }
    }
}